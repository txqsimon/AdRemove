{
	"name" : "ad_remove",
	"manifest_version" : 2,
	"version" : "1.0.0",
	"description" : "remove advertisements in web",
	"author" : "simon",
	"browser_action" : {
		"default_icon" : "img/logo2.jpg"
	},
	"content_scripts" : [
		{
			"matches" : [
				"*://*/*"
			],
			"js" : [
				"src/js/jquery.min.js",
				"src/js/ad_remove.js"
			],
			"all_frames" : true
		}
	]

}